{
  "name": "react-tc39-demo",
  "version": "1.0.0",
  "description": "Demo for react and tc39 features",
  "keywords": [
    "react",
    "tc39",
    "demo"
  ],
  "license": "MIT",
  "homepage": "https://putstrln.github.io/react-tc39-demo/",
  "repository": {
    "type": "git",
    "url": "https://github.com/putstrln/react-tc39-demo.git"
  },
  "scripts": {
    "build": "yarn clean-dist && webpack --config=configs/webpack/prod.js",
    "build:prod": "yarn clean-dist && yarn build",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start": "yarn start-dev",
    "start-dev": "webpack serve --config=configs/webpack/dev.js",
    "start-prod": "yarn build:prod && node express.js",
    "test": "jest --coverage --watchAll --config=configs/jest.json",
    "deploy": "BASE_URL='/react-tc39-demo' yarn build:prod && cp dist/index.html dist/404.html && gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@testing-library/react": "^13.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-loader": "^8.2.4",
    "babel-plugin-import-remove-resource-query": "^1.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "express": "^4.17.3",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.6",
    "gh-pages": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "jest": "^27.5.1",
    "node-sass": "^7.0.1",
    "prettier": "^2.6.2",
    "raw-loader": "^4.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-refresh": "^0.12.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react-code-blocks": "^0.0.9-0",
    "react-router-dom": "^6.3.0"
  }
}
